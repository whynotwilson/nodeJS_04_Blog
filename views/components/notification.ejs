<div class="ui grid">
  <div class="four wide colume"></div>
  <div class="eight wide colume">
    <% if (success) { %>
      <div class="ui success message">
        <p><%= success %></p>
      </div>
    <% } %>

    <% if (error) { %>
      <div class="ui error message">
        <p><%= error %></p>
      </div>
    <% } %>
  </div>
</div>
